{
    "name": "goetu/messenger",
    "description": "A package for Laravel PHP Framework to add a complete real-time chat system.",
    "type": "library",
    "keywords": ["laravel", "messenger"],
    "license": "MIT",
    "authors": [
        {
            "name": "GoETU Infotech",
            "email": "ralphyi@goetu.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "pusher/pusher-php-server": "^3.4@dev"
    },
    "autoload": {
        "psr-4": {
            "Chatify\\" : "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chatify\\ChatifyServiceProvider"
            ],
            "aliases": {
                "Chatify": "Chatify\\Facades\\ChatifyMessenger"
            }
        }
    }
}
